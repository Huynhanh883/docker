{
  "network": "livenet",
  "port": 80,
  "services": [
    "bitcoind",
    "insight-api-zcash",
    "insight-ui-zcash",
    "web"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "/mnt/data",
        "exec": "/opt/zcash/src/zcashd"
      }
    },
    "insight-ui-zcash": {
      "apiPrefix": "api",
      "routePrefix": ""
    },
    "insight-api-zcash": {
      "routePrefix": "api",
      "disableRateLimiter": true,
      "rateLimiterOptions": {
        "whitelistLimit": 2147483647,
        "limit": 2147483647
      }
    }
  }
}