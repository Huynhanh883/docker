{
  "network": "livenet",
  "port": 80,
  "services": [
    "bitcoind",
    "insight-api-anon",
    "insight-ui-anon",
    "web"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "/mnt/data",
        "exec": "/opt/anon/src/anond"
      }
    },
    "insight-ui-anon": {
      "apiPrefix": "api",
      "routePrefix": ""
    },
    "insight-api-anon": {
      "routePrefix": "api",
      "disableRateLimiter": true,
      "rateLimiterOptions": {
        "whitelistLimit": 2147483647,
        "limit": 2147483647
      }
    }
  }
}